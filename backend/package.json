{
  "name": "soccer-stats-backend",
  "version": "1.0.0",
  "description": "FIFA Players Stats Manager - Backend",
  "main": "dist/index.js",
  "scripts": {
    "start": "bun --watch src/index.ts",
    "start:docker": "bun run start",
    "build": "bun build src/index.ts --outdir ./dist --target node",
    "build:prod": "bun run build",
    "serve": "bun run dist/index.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "check-custom-player": "ts-node scripts/checkCustomPlayer.ts"
  },
  "keywords": [
    "fifa",
    "stats",
    "soccer"
  ],
  "author": "Emmanuel Van Dick",
  "license": "MIT",
  "dependencies": {
    "@types/uuid": "^11.0.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.13.0",
    "bcrypt": "6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "fast-csv": "^5.0.5",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "18.0.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "latest",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.7.2",
    "bun-types": "^1.3.0",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
